{
  "name": "word-regex",
  "description": "Regular expression for matching words in a string. Support for english, CJK and Cyrillic.",
  "version": "0.1.2",
  "homepage": "https://github.com/regexps/word-regex",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "regexps/word-regex",
  "bugs": {
    "url": "https://github.com/regexps/word-regex/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.11",
    "mocha": "^3.2.0"
  },
  "keywords": [
    "count",
    "detect",
    "exec",
    "expression",
    "find",
    "match",
    "regex",
    "regexp",
    "regular",
    "split",
    "test",
    "word",
    "words"
  ],
  "verb": {
    "related": {
      "list": [
        "copyright-regex",
        "is-equal-regex",
        "todo-regex",
        "whitespace-regex"
      ]
    },
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "verb",
      "verb-generate-readme"
    ]
  }
}
